{
  "$schema": "http://json.schemastore.org/template",
  "author": "Jason Benfield",
  "classifications": [ "Common", "Library" ],
  "identity": "XTI_Templates.WebApp",
  "name": "XTI_Templates.WebApp",
  "shortName": "xtiwebapp",
  "tags": {
    "type": "project",
    "language": "en-US"
  },
  "placeholderFilename": ".keep",
  "symbols": {
    "AppName": {
      "type": "parameter",
      "datatype": "string",
      "isRequired": true,
      "defaultValue": "TheApp",
      "FileRename": "AppName",
      "replaces": "__APPNAME__"
    },
    "AppNameLower": {
      "type": "parameter",
      "datatype": "string",
      "isRequired": true,
      "defaultValue": "TheApp",
      "FileRename": "AppNameLower",
      "replaces": "__APPNAMELOWER__"
    },
    "RepoOwner": {
      "type": "parameter",
      "datatype": "string",
      "isRequired": true,
      "defaultValue": "TheRepoOwner",
      "FileRename": "RepoOwner",
      "replaces": "__REPOOWNER__"
    },
    "RepoName": {
      "type": "parameter",
      "datatype": "string",
      "isRequired": true,
      "defaultValue": "TheRepoName",
      "FileRename": "Domain",
      "replaces": "__REPONAME__"
    },
    "Domain": {
      "type": "parameter",
      "datatype": "string",
      "isRequired": true,
      "defaultValue": "development.guinevere.com",
      "FileRename": "Domain",
      "replaces": "__DOMAIN__"
    }
  }
}