{
  "$schema": "http://json.schemastore.org/template",
  "author": "Jason Benfield",
  "classifications": [ "Common", "Library" ],
  "identity": "XTI_Templates.XtiSolution",
  "name": "XTI_Templates.XtiSolution",
  "shortName": "xtisolution",
  "tags": {
    "type": "project",
    "language": "en-US"
  },
  "symbols": {
    "RepoName": {
      "type": "parameter",
      "datatype": "string",
      "isRequired": true,
      "defaultValue": "TheRepoName",
      "FileRename": "RepoName",
      "replaces": "__REPONAME__"
    },
    "RepoOwner": {
      "type": "parameter",
      "datatype": "string",
      "isRequired": false,
      "FileRename": "RepoOwner",
      "replaces": "__REPOOWNER__"
    },
    "XtiOverrides": {
      "type": "parameter",
      "datatype": "string",
      "defaultValue": "xti.private",
      "FileRename": "xtioverrides"
    }
  }
}